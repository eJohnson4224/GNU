{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-fsanitize=address",
        "-O0",
        "-Wall",
        "-Wextra",
        "-Werror",
        "-D",
        "BUFFER_SIZE=1098098",
        "-g",
        "${fileDirname}/*.c",
        "-o",
        "${fileDirname}/${fileDirnameBasename}"
      ],
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}